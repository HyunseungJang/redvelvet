<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".MyInfoMainActivity">

    <TextView
        android:id="@+id/outputBirth2"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="100dp"
        android:layout_marginEnd="106dp"
        android:layout_marginBottom="14dp"
        android:hint="00세"
        android:textSize="16sp"
        app:layout_constraintBottom_toTopOf="@+id/scrollView2"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toEndOf="@+id/textView3"
        app:layout_constraintTop_toBottomOf="@+id/outputName" />

    <TextView
        android:id="@+id/textView3"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="18dp"
        android:layout_marginTop="36dp"
        android:layout_marginEnd="268dp"
        android:layout_marginBottom="24dp"
        android:text="마이페이지"
        android:textColor="#000000"
        android:textSize="34sp"
        android:textStyle="bold"
        app:layout_constraintBottom_toTopOf="@+id/linearLayout2"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.064"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.0" />

    <Button
        android:id="@+id/updateButton"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="17dp"
        android:layout_marginEnd="40dp"
        android:layout_marginBottom="52dp"
        android:backgroundTint="#F26161"
        android:text="수정 하러 가기"
        android:textColor="#FFFFFF"
        android:textSize="16sp"
        android:textStyle="bold"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/scrollView2" />

    <TextView
        android:id="@+id/outputName"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginEnd="48dp"
        android:layout_marginBottom="44dp"
        android:hint="00님의 정보"
        android:textColor="#F26161"
        android:textSize="20sp"
        android:textStyle="bold"
        app:layout_constraintBottom_toTopOf="@+id/scrollView2"
        app:layout_constraintEnd_toEndOf="parent" />

    <ScrollView
        android:id="@+id/scrollView2"
        android:layout_width="355dp"
        android:layout_height="507dp"
        android:layout_marginTop="26dp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/textView3"
        app:layout_constraintVertical_bias="0.0">

        <LinearLayout
            android:id="@+id/linearLayout3"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:orientation="vertical">

            <TextView
                android:id="@+id/textView5"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:text="의학적 질환"
                android:textColor="#F26161"
                android:textSize="16sp"
                android:textStyle="bold"
                app:layout_constraintStart_toStartOf="parent" />

            <TextView
                android:id="@+id/outputSick"
                android:layout_width="match_parent"
                android:layout_height="40dp"
                android:hint="의학적 질환"
                android:textSize="20sp" />

            <TextView
                android:id="@+id/textView6"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:text="긴급 연락처"
                android:textColor="#F26161"
                android:textSize="16sp"
                android:textStyle="bold" />

            <TextView
                android:id="@+id/outputEmer"
                android:layout_width="match_parent"
                android:layout_height="40dp"
                android:hint="긴급 연락처"
                android:textSize="20sp" />

            <TextView
                android:id="@+id/textView8"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:text="혈액형"
                android:textColor="#F26161"
                android:textSize="16sp"
                android:textStyle="bold" />

            <TextView
                android:id="@+id/outputBlood"
                android:layout_width="match_parent"
                android:layout_height="40dp"
                android:hint="혈액형"
                android:textSize="20sp" />

            <TextView
                android:id="@+id/textView13"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:text="복용 중인 약"
                android:textColor="#F26161"
                android:textSize="16sp"
                android:textStyle="bold" />

            <TextView
                android:id="@+id/outputMedi"
                android:layout_width="match_parent"
                android:layout_height="40dp"
                android:hint="복용 중인 약"
                android:textSize="20sp" />

            <TextView
                android:id="@+id/textView20"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:text="신장"
                android:textColor="#F26161"
                android:textSize="16sp"
                android:textStyle="bold" />

            <TextView
                android:id="@+id/outputTall"
                android:layout_width="match_parent"
                android:layout_height="40dp"
                android:hint="신장"
                android:textSize="20sp" />

            <TextView
                android:id="@+id/textView21"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:text="체중"
                android:textColor="#F26161"
                android:textSize="16sp"
                android:textStyle="bold" />

            <TextView
                android:id="@+id/outputWeight"
                android:layout_width="match_parent"
                android:layout_height="40dp"
                android:hint="체중"
                android:textSize="20sp" />

            <TextView
                android:id="@+id/textView22"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:text="자격증"
                android:textColor="#F26161"
                android:textSize="16sp"
                android:textStyle="bold" />

            <TextView
                android:id="@+id/outputCertifi"
                android:layout_width="match_parent"
                android:layout_height="40dp"
                android:hint="자격증"
                android:textSize="20sp" />

            <TextView
                android:id="@+id/textView23"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:text="기타 사항"
                android:textColor="#F26161"
                android:textSize="16sp"
                android:textStyle="bold" />

            <TextView
                android:id="@+id/outputOther"
                android:layout_width="match_parent"
                android:layout_height="40dp"
                android:hint="기타"
                android:textSize="20sp" />//
        </LinearLayout>
    </ScrollView>

</androidx.constraintlayout.widget.ConstraintLayout>